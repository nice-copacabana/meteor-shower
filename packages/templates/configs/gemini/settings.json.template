{
  "model": {
    "name": "{{geminiModel}}",
    "temperature": {{temperature}}
  },
  "tools": {
    "allowed": [
      "run_shell_command(git status)",
      "run_shell_command(npm test)",
      "run_shell_command(npm run build)"
    ]
  },
  "context": {
    "fileName": "GEMINI.md",
    "fileFiltering": {
      "respectGitIgnore": true,
      "respectGeminiIgnore": true
    }
  },
  "project": {
    "name": "{{projectName}}",
    "description": "{{projectDescription}}"
  }
}
